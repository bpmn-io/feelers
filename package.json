{
  "name": "feelers",
  "version": "1.4.0",
  "description": "FEELers grammar and editor for the Lezer parser system.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "scripts": {
    "all": "run-s lint test build",
    "test:headless": "vitest run --project node",
    "test": "vitest run",
    "generate:parser": "lezer-generator src/grammar/feelers.grammar -o src/grammar/parser",
    "bundle": "vite build",
    "dev": "vitest watch",
    "build": "npm run generate:parser && npm run bundle",
    "lint": "eslint .",
    "start": "vitest singleStart --ui",
    "build:playground": "npm run build && npm run --prefix feelers-playground build",
    "start:playground": "npm run build && npm run --prefix feelers-playground dev"
  },
  "author": {
    "name": "Valentin Serra",
    "url": "https://github.com/skaiir"
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "keywords": [
    "FEEL",
    "FEELers",
    "Lezer",
    "Grammer",
    "Parser"
  ],
  "files": [
    "dist"
  ],
  "license": "MIT",
  "dependencies": {
    "@bpmn-io/cm-theme": "^0.1.0-alpha.2",
    "@bpmn-io/feel-lint": "^3.0.0",
    "@codemirror/autocomplete": "^6.10.1",
    "@codemirror/commands": "^6.3.0",
    "@codemirror/language": "^6.9.1",
    "@codemirror/lint": "^6.4.2",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.21.3",
    "@lezer/common": "^1.1.0",
    "@lezer/highlight": "^1.1.6",
    "@lezer/lr": "^1.3.13",
    "@lezer/markdown": "^1.1.0",
    "feelin": "^5.0.0",
    "lezer-feel": "^2.0.0",
    "min-dom": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@lezer/generator": "^1.5.1",
    "@vitest/browser-preview": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.39.1",
    "eslint-plugin-bpmn-io": "^2.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.5.0",
    "jsdom": "^27.3.0",
    "npm-run-all2": "^8.0.0",
    "typescript": "^4.9.5",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  },
  "workspaces": {
    "packages": [
      "feelers-playground"
    ]
  }
}
